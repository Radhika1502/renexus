# Database
DATABASE_URL=postgresql://postgres:root@localhost:5432/renexus

# Server
PORT=3000
NODE_ENV=development

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:3001

# Security
RATE_LIMIT_WINDOW_MS=15 * 60 * 1000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info

# API Keys (if any)
# EXTERNAL_API_KEY=your_api_key_here
